@Article{Bertin2016,
  author       = {Bertin, Marc and Atanassova, Iana and Larivière, Vincent and Gingras, Yves},
  date         = {2016},
  journaltitle = {Journal of the Association for Information Science and Technology (JASIST)},
  title        = {The Invariant Distribution of References in Scientific Articles},
  doi          = {10.1002/asi.23367},
  issn         = {2330-1643},
  number       = {1},
  pages        = {164-177},
  volume       = {67},
  abstract     = {The organization of scientific papers typically follows a standardized pattern, the well-known IMRaD structure (Introduction, Methods, Results, and Discussion). Using the full text of 45,000 papers published in the PLoS series of journals as a case study, this paper investigates, from the viewpoint of bibliometrics, how references are distributed along the structure of scientific papers, as well as the age of these cited references. It shows that, once the sections of articles are realigned to follow the IMRaD sequence, the position of cited references along the text of articles is invariant across all PLoS journals, with the introduction and conclusion accounting for most of the references. It also provides evidence that the age of cited references varies quite a lot by section, with older references being found in the methods and more recent ones in the discussion. On the whole, these results provide insight into the different roles citations have in the scholarly communication process.},
  author+an    = {2=me},
  bibtex_show  = {true},
  annotation      = {revue-com-lecture},
}

